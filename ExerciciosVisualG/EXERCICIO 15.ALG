algoritmo "Exercicio 15"
var
    dias, HorasT : Inteiro
    salario: Real
inicio
    Escreva("Quantos dias foram trabalhados no mês ? ")
    Leia(dias)
    HorasT <- dias * 8
    salario <- HorasT * 25
    Escreva("O sálario equivalente as horas trabalhadas em ",dias," dias é de R$",salario:7:2)
    
    
    
fimalgoritmo